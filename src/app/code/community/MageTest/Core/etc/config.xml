<?xml version="1.0"?>
<config>
    <modules>
        <MageTest_Core>
            <version>0.1.12</version>
            <depends>
                <Mage_Core />
            </depends>
        </MageTest_Core>
    </modules>
    <global>
        <models>
            <admin>
                <rewrite>
                    <session>MageTest_Core_Model_Admin_Session</session>
                </rewrite>
            </admin>
            <core>
                <rewrite>
                    <email_template>MageTest_Core_Model_Email_Template</email_template>
                </rewrite>
            </core>
        </models>
        <helpers>
            <core>
                <rewrite>
                    <http>MageTest_Core_Helper_Http</http>
                </rewrite>
            </core>
        </helpers>
    </global>
</config>